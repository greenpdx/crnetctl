[connection]
id=MyVPN
uuid=87654321-4321-4321-4321-cba987654321
type=vpn
autoconnect=false

[vpn]
service-type=org.freedesktop.NetworkManager.openvpn
connection-type=tls
remote=vpn.example.com:1194
ca=/etc/openvpn/ca.crt
cert=/etc/openvpn/client.crt
key=/etc/openvpn/client.key
dev=tun

[ipv4]
method=auto
never-default=false

[ipv6]
method=auto
